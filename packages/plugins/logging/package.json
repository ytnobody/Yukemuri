{
  "name": "@yukemuri/plugin-logging",
  "version": "1.0.0",
  "description": "Structured logging plugin for Yukemuri - Multi-sink support with log levels",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "tsx --test tests/**/*.test.ts",
    "test:watch": "tsx --test --watch tests/**/*.test.ts"
  },
  "keywords": [
    "yukemuri",
    "plugin",
    "logging",
    "structured-logging",
    "log-levels",
    "multi-sink"
  ],
  "author": "Yukemuri Team",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@yukemuri/core": "workspace:*",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0"
  },
  "peerDependencies": {
    "@yukemuri/core": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0",
    "yukemuri": "^1.0.0"
  }
}
